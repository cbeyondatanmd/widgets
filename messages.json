{
    "id": "com.cbeyondata.messages",
    "version": "1.0.0",
    "name": "BAS Messages",
    "description": "BAS Messages",
    "newInstancePrefix": "Messages",
    "icon": "data:image/svg+xml;base64,",
    "vendor": "cBeyondata",
    "eula": "",
    "license": "",
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-cbeyondata-messages",
            "url": "/messages.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {},
    "types": {
        "message": {
            "description": "Message.",
            "properties": [
                {
                    "name": "content",
                    "type": "Object<Selection>",
                    "description": "Message Content."
                }
            ]
        }
    },
    "methods": {
        "sendMessage": {
            "description": "Send Message to registered listener.",
            "parameters": [
                {
                    "name": "id",
                    "type": "string",
                    "description": "Listener ID to send Message to."
                },
                {
                    "name": "message",
                    "type": "message",
                    "description": "Message to send."
                }
            ]
        },
        "getMessage": {
            "returnType": "message",
            "description": "Get Message."
        },
        "register": {
            "description": "Register for messages.",
            "parameters": [
                {
                    "name": "id",
                    "type": "string",
                    "description": "Listener ID to send Message to."
                }
            ]
        },
        "initMessage": {
            "returnType": "message",
            "description": "Used to Initailize Message Variable."
        }
    },
    "events": {
        "onClick": {
            "description": "Called when the user clicks the Sidebar."
        },
        "messageReceived": {
            "description": "Called when the user clicks the Sidebar."
        }
    }
}